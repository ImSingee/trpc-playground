{
  "name": "trpc-playground",
  "type": "module",
  "private": true,
  "scripts": {
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "pnpm -r type-check",
    "lint:js": "eslint .",
    "prepare": "husky install",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "dev": "pnpm dev --filter next-app"
  },
  "devDependencies": {
    "@swc/core": "1.2.121",
    "@swc/jest": "0.2.15",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.1",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "dprint": "0.19.1",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "lint-staged": "12.1.3",
    "npm-run-all": "4.1.5",
    "preact": "10.6.4",
    "preact-ssr-prepass": "1.2.0",
    "regenerator-runtime": "0.13.9",
    "tsup": "5.11.6",
    "typescript": "4.5.4"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{js,ts,md,json,yaml}": "dprint fmt"
  },
  "pnpm": {
    "overrides": {
      "react-ssr-prepass": "npm:preact-ssr-prepass@1.2.0"
    }
  }
}
