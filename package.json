{
  "name": "trpc-playground",
  "type": "module",
  "private": true,
  "scripts": {
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "pnpm -r type-check",
    "lint:js": "eslint .",
    "prepare": "husky install",
    "test": "vitest",
    "dev": "pnpm dev --filter next-app"
  },
  "devDependencies": {
    "@types/node": "17.0.4",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "dprint": "0.19.2",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "nano-staged": "0.5.0",
    "npm-run-all": "4.1.5",
    "preact": "10.6.4",
    "preact-ssr-prepass": "1.2.0",
    "tsup": "5.11.9",
    "typescript": "4.5.4",
    "vite": "2.7.6",
    "vitest": "0.0.113"
  },
  "sideEffects": false,
  "nano-staged": {
    "*.{js,ts,md,json,yaml}": "dprint fmt"
  },
  "pnpm": {
    "overrides": {
      "react-ssr-prepass": "npm:preact-ssr-prepass@1.2.0"
    }
  }
}
