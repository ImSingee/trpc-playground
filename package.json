{
  "name": "root",
  "type": "module",
  "private": true,
  "scripts": {
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "turbo run type-check",
    "lint:js": "eslint .",
    "build": "turbo run build --scope=\"@trpc-playground/**\" --scope=\"trpc-playground\" --no-deps",
    "clean": "pnpm -r clean",
    "test": "vitest",
    "dev:vite-server": "pnpm  --filter vite-app dev:server",
    "dev:vite-client": "pnpm  --filter vite-app dev:client",
    "dev:serve-html": "pnpm --filter @trpc-playground/html serve-dev",
    "dev": "run-p dev:*"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.7",
    "@changesets/cli": "2.25.2",
    "@types/node": "18.11.17",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "dprint": "0.33.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "4.1.5",
    "preact": "10.11.3",
    "rimraf": "3.0.2",
    "tsup": "6.5.0",
    "turbo": "1.6.3",
    "typescript": "4.9.4"
  },
  "sideEffects": false,
  "pnpm": {
    "packageExtensions": {
      "@lezer/javascript": {
        "dependencies": {
          "@lezer/common": "*"
        }
      }
    }
  },
  "packageManager": "pnpm@7.9.0"
}
