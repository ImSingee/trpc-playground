{
  "name": "@trpc-playground/typescript-extension",
  "version": "0.0.0",
  "description": "tsserver support for codemirror",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist src/types",
    "prepublishOnly": "pnpm build",
    "prepare": "pnpm build-types",
    "build-types": "node ./scripts/build-types.js",
    "type-check": "tsc"
  },
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true
  },
  "dependencies": {
    "@codemirror/autocomplete": "0.19.9",
    "@codemirror/lang-javascript": "0.19.3",
    "@codemirror/lint": "0.19.3",
    "@codemirror/state": "0.19.6",
    "@codemirror/tooltip": "0.19.10",
    "@codemirror/view": "0.19.37",
    "@types/node": "17.0.5",
    "@typescript/vfs": "1.3.5",
    "localforage": "1.10.0",
    "lodash": "4.17.21",
    "typescript": "4.5.4"
  },
  "devDependencies": {
    "fast-glob": "3.2.7"
  }
}
