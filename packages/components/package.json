{
  "name": "@trpc-playground/components",
  "type": "module",
  "version": "0.0.0",
  "description": "core components for trpc-playground",
  "repository": {
    "directory": "packages/core",
    "type": "git",
    "url": "https://github.com/sachinraja/trpc-playground"
  },
  "homepage": "https://github.com/sachinraja/trpc-playground#readme",
  "bugs": {
    "url": "https://github.com/sachinraja/trpc-playground/issues"
  },
  "author": "Sachin Raja <sachinraja2349@gmail.com>",
  "license": "MIT",
  "keywords": [
    "trpc",
    "playground"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "./playground": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./handlers/next": {
      "import": "./dist/handlers/next.js",
      "default": "./dist/handlers/next.cjs"
    },
    "./css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --no-dts",
    "type-check": "tsc"
  },
  "dependencies": {
    "@codemirror/basic-setup": "0.19.1",
    "@codemirror/history": "0.19.1",
    "@codemirror/lang-javascript": "0.19.3",
    "@codemirror/lang-json": "0.19.1",
    "@codemirror/state": "0.19.6",
    "@codemirror/theme-one-dark": "0.19.1",
    "@codemirror/view": "0.19.37",
    "@heroicons/react": "1.0.5",
    "@trpc/react": "9.16.0",
    "clsx": "1.1.1",
    "jotai": "1.5.0",
    "object-inspect": "1.12.0",
    "react-query": "3.34.7",
    "rodemirror": "1.6.6"
  },
  "peerDependencies": {
    "preact": "^10"
  },
  "devDependencies": {
    "@types/object-inspect": "1.8.1",
    "@types/react": "17.0.38",
    "autoprefixer": "10.4.1",
    "postcss": "8.4.5",
    "postcss-prefix-selector": "1.14.0",
    "tailwindcss": "3.0.8"
  }
}
