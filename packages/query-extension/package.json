{
  "name": "@trpc-playground/query-extension",
  "version": "0.0.0",
  "type": "module",
  "description": "parse query syntax for trpc-playground",
  "license": "Apache-2.0",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "type-check": "tsc",
    "prepublishOnly": "pnpm build"
  },
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true
  },
  "dependencies": {
    "@codemirror/gutter": "^0.19.9",
    "@codemirror/language": "^0.19.7",
    "@codemirror/rangeset": "^0.19.6",
    "@codemirror/state": "^0.19.6",
    "@codemirror/view": "^0.19.39",
    "relaxed-json": "^1.0.3"
  },
  "devDependencies": {
    "@types/lodash": "4.14.178",
    "@types/relaxed-json": "1.0.1",
    "lodash": "4.17.21"
  }
}
